{
  "name": "spree_google_analytics",
  "version": "0.1.8",
  "description": "Spree Google Analytics",
  "main": "package/dist/index.umd.js",
  "module": "package/dist/index.esm.js",
  "unpkg": "package/dist/index.min.js",
  "browser": {
    "./sfc": "package/src/index.js"
  },
  "files": [
    "package/dist/**/*"
  ],
  "directories": {
    "lib": "package/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upsidelab/spree_google_analytics.git"
  },
  "keywords": [],
  "author": "Upsidelab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/upsidelab/spree_google_analytics/issues"
  },
  "homepage": "https://github.com/upsidelab/spree_google_analytics#readme",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file package/dist/index.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file package/dist/index.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file package/dist/index.min.js"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup": "^1.17.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue-template-compiler": "^2.6.10"
  }
}
